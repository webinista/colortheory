{
  "name": "colors",
  "version": "0.0.1",
  "description": "A color scheme creator.",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack -p --config config/webpack.development.js",
    "build:prod": "webpack -p --config config/webpack.prod.js",
    "lint:js": "eslint --quiet --ext .js --ext .jsx .",
    "test": "mocha --recursive --compilers js:babel-core/register './test/**/*.unit.test.js?(x)'",
    "test:components": "mocha --recursive --compilers js:babel-core/register './test/components/*.unit.test.jsx'",
    "test:validators": "mocha --recursive --compilers js:babel-core/register './test/utils/validators.unit.test.js'",
    "test:helpers": "mocha --recursive --compilers js:babel-core/register './test/utils/helpers.*.unit.test.js'",
    "test:conversions": "mocha --recursive --compilers js:babel-core/register './test/utils/conversions.*.unit.test.js'",
    "test:colorschemes": "mocha --recursive --compilers js:babel-core/register './test/utils/colorschemes.*.unit.test.js'",
    "watch": "webpack-dev-server --open --config config/webpack.development.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webinista/colors.git"
  },
  "author": "Tiffany B. Brown, Webinista, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webinista/colors/issues"
  },
  "engines": {
    "node": "6.10.3",
    "npm": "5.0.0"
  },
  "homepage": "https://github.com/webinista/colors#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-es5": "^0.5.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.2",
    "react-test-renderer": "^15.5.4",
    "sass-lint": "^1.10.2",
    "skin-deep": "^1.0.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  }
}
